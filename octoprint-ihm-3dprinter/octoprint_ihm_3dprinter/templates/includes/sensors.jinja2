<div hidden class="pluginViewContainer sensorsContainer">

  <h4>Filament Temperature</h4>

  <div class="row-fluid" id="sensor-filament-temperature-overview">

    <div class="span6">
        <div class="detail-plot-container">
            <div class="detail-plot" id="FLIRSensorChart"></div>
        </div>
    </div>

    <div class="span6">
        <table class="table table-bordered table-condensed">
            <tbody>

              <tr data-bind="css: {success: available(), error: !available()}">
                <td>Available</td>
                <td><span data-bind="text: available() ? 'YES': 'NO'"></span></td>
              </tr>

              <tr data-bind="css: {error: rangeError}">
                <td>Minimum [<span data-bind="text: unit()"></span>]</td>
                <td><span data-bind="text: minimum() !== null ? minimum() : 'Not set'"></span></td>
              </tr>

              <tr data-bind="css: {error: rangeError}">
                <td>Maximum [<span data-bind="text: unit()"></span>]</td>
                <td><span data-bind="text: maximum() !== null ? maximum() : 'Not set'"></span></td>
              </tr>

              <tr>
                <td>Offset [<span data-bind="text: unit()"></span>]</td>
                <td><span data-bind="text: offset"></span></td>
              </tr>

              <tr>
                <td>Coefficient</td>
                <td><span data-bind="text: coefficient"></span></td>
              </tr>

            </tbody>
        </table>
    </div>

  </div>

  <div class="row-fluid">

      <div class="span2 offset2">
        Minimum
      </div>

      <div class="span2">
        Maximum
      </div>

      <div class="span2">
        Offset
      </div>

      <div class="span2">
        Coefficient
      </div>

  </div>
  <div class="row-fluid" id="sensor-filament-temperature-control">

    <div class="span2">
      Parameters
    </div>

    <div class="span2">
      <input type="text" class="input-block-level" data-bind="value: newMinimum">
    </div>

    <div class="span2">
      <input type="text" class="input-block-level" data-bind="value: newMaximum">
    </div>

    <div class="span2">
      <input type="text" class="input-block-level" data-bind="value: newOffset">
    </div>

    <div class="span2">
      <input type="text" class="input-block-level" data-bind="value: newCoefficient">
    </div>

    <button type="button" class="btn span2" data-bind="click: setParameters.bind($data)">
        <i class="icon-ok"></i> Apply
    </button>

  </div>

  <hr>

